generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model skins {
  id            Int     @id
  src           String  @db.VarChar(255)
  correctanswer String  @db.VarChar(255)
  rarity        String? @db.VarChar(255)
}
